<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.murilo.minecraft">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Minecraft">
        <activity
            android:name=".PotionImmediateDamageActivity"
            android:exported="false" />
        <activity
            android:name=".PotionWeaknessActivity"
            android:exported="false" />
        <activity
            android:name=".PotionPoisonActivity"
            android:exported="false" />
        <activity
            android:name=".PotionInvisibleActivity"
            android:exported="false" />
        <activity
            android:name=".PotionSuperJumpActivity"
            android:exported="false" />
        <activity
            android:name=".PotionFireResistense"
            android:exported="false" />
        <activity
            android:name=".PotionNightVision"
            android:exported="false" />
        <activity
            android:name=".PotionStrenghtActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PositivePotionsActivity" />
        <activity android:name=".NegativePotionsActivity" />
    </application>

</manifest>